<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
			    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
			    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.xpy.domain.Supply" table="sms_supply">
		<id name="supId" column="supId">
			<generator class="native"></generator>
		</id>
		<version name="version"></version>
		<property name="realStock" column="realStock"></property>
		<property name="MinStock" column="MinStock"></property>
		<property name="goodId" column="goodId"></property>
		<property name="proId" column="proId"></property>
	</class>
</hibernate-mapping>