<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
			    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
			    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.xpy.domain.Order" table="sms_order">
		<id name="orderId" column="orderId">
			<generator class="native"></generator>
		</id>
		<version name="version"></version>
		<property name="userId" column="userId"></property>
		<property name="goodId" column="goodId"></property>
		<property name="rAddress" column="rAddress"></property>
		<property name="spDate" column="spDate"></property>
		<property name="spCount" column="spCount"></property>
		<property name="gPrice" column="gPrice"></property>
		<property name="receiptID" column="receiptID"></property>
		<property name="spPrice" column="spPrice"></property>
		<property name="sendTime" column="sendTime"></property>
		<property name="payment" column="payment"></property>
		<property name="sendType" column="sendType"></property>
		<property name="sendPrice" column="sendPrice"></property>
		<property name="state" column="state"></property>
	</class>
</hibernate-mapping>